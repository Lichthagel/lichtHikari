{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "target": "ES2022",
        "types": [
            "unplugin-icons/types/solid"
        ]
    }
}
